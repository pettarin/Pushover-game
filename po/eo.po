#: po/leveltexts.txt:142
msgid "040"
msgstr "040"

#: po/leveltexts.txt:143
msgid "Move the Delay!"
msgstr "Movigu la Prokrastanton!"

#: po/leveltexts.txt:144
msgid "041"
msgstr "041"

#: po/leveltexts.txt:145
msgid "Build a bridge!"
msgstr "Konstruu ponton!"

#: po/leveltexts.txt:146
msgid "042"
